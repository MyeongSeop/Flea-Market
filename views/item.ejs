    <script>
        function upload(){
            var question = document.getElementById("question");
            if(!question.value) alert("Please Enter Question!");
            else{
                var itemID = document.getElementById("btn-q").value;
                var gogo = question.value;
                var cur_url = document.location.href;
                gogo = "/q_upload?q=" + gogo + "&itemID=" + itemID+"&cur="+cur_url;
                window.location.href = gogo;
            }
            
        }

        function answer(){
            var answer = document.getElementById("answer");
            var q_num = document.getElementById("q-num");
            if(!answer.value || !q_num.value) alert("Please Enter Answer!");
            else{
                var itemID = document.getElementById("btn-a").value;
                var gogo = answer.value;
                var cur_url = document.location.href;
                q_num = q_num.value;
                gogo = "/a_upload?a=" + gogo + "&itemID=" + itemID + "&cur=" + cur_url + "&q_num=" + q_num;
                window.location.href = gogo;
            }
        }
        

        function bid_upload(){
            var myID = document.getElementById("btn-like");
            if(!myID.value || myID.value==''){
                alert("Please Login First!");
                window.location.href = "/login";
            }
            else{
                myID = myID.value;
                var time_left = document.getElementById("time_left").value;
                time_left = Number(time_left);
                if(time_left <= 0){
                    alert("Bidding is done!");
                    return;
                }
                var sellerID = document.getElementById("bid-upload").value;
                if(myID == sellerID){
                    alert("It's your product!");
                    return;
                }
                var bid_price = document.getElementById("bid-price");
                var cur_price = document.getElementById("btn-bid").value;
                var max_price = document.getElementById('btn-imm').value;
                cur_price = Number(cur_price);
                max_price = Number(max_price);
                if(!bid_price.value || Number(bid_price.value) <= cur_price || Number(bid_price.value) >= max_price){
                    alert("Please Enter Proper Price");
                    return;
                }
                bid_price = bid_price.value;
                
                var ischeck = $(bid_check).is(":checked");
